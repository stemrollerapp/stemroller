{
  "name": "stemroller",
  "version": "2.1.2",
  "private": true,
  "type": "module",
  "description": "Isolate vocals, drums, bass, and other instrumental stems from any song",
  "homepage": "https://stemroller.com",
  "license": "Unlicense",
  "author": "StemRoller Developers",
  "repository": "https://github.com/stemrollerapp/stemroller.git",
  "main": "main-src/main.cjs",
  "scripts": {
    "download-third-party-apps": "node download-third-party-apps.js",
    "dev": "cross-env NODE_ENV=dev STEMROLLER_RUN_FROM_SOURCE=true npm run dev:all",
    "dev:all": "concurrently -k -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
    "dev:svelte": "vite dev",
    "dev:electron": "electron .",
    "build:mac": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron:mac",
    "build:win": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron:win",
    "build:svelte": "vite build",
    "build:electron:mac": "electron-builder -m --config electron-builder.config.json",
    "build:electron:win": "electron-builder -w --config electron-builder.config.json",
    "start": "cross-env NODE_ENV=production STEMROLLER_RUN_FROM_SOURCE=true npm run start:all",
    "start:all": "concurrently -k -n=svelte,electron -c='#ff3e00',blue \"npm run start:svelte\" \"npm run start:electron\"",
    "start:svelte": "vite preview",
    "start:electron": "electron ."
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "^1.0.0-next.357",
    "7zip-bin": "^5.2.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "electron": "^31.3.1",
    "electron-builder": "^23.1.0",
    "electron-connect": "^0.6.3",
    "electron-packager": "^15.5.1",
    "electron-updater": "^5.0.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "extract-zip": "^2.0.1",
    "heroicons": "^1.0.6",
    "lodash.debounce": "^4.0.8",
    "node-7z": "^3.0.0",
    "node-fetch": "^3.2.10",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.4"
  },
  "dependencies": {
    "@distube/ytdl-core": "^4.16.8",
    "compare-versions": "^4.1.3",
    "electron-fetch": "^1.7.4",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.2",
    "sanitize-filename": "^1.6.3",
    "tree-kill": "^1.2.2",
    "undici": "^6.21.2",
    "xxhash-wasm": "^1.0.1",
    "yt-search": "^2.10.4"
  }
}
