{
	"name": "stemroller",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"description": "Isolate vocals, drums, bass, and other instrumental stems from any song",
	"homepage": "https://stemroller.com",
	"license": "Unlicense",
	"author": "StemRoller Developers",
	"repository": "https://github.com/stemrollerapp/stemroller.git",
	"main": "main-src/main.cjs",
	"scripts": {
		"dev": "cross-env NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c='#ff3e00',blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "svelte-kit dev",
		"dev:electron": "electron .",
		"build:mac": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron:mac",
		"build:win": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron:win",
		"build:linux": "cross-env NODE_ENV=production npm run build:svelte && npm run build:electron:linux",
		"build:svelte": "svelte-kit build",
		"build:electron:mac": "electron-builder -m --config electron-builder.config.json",
		"build:electron:win": "electron-builder -w --config electron-builder.config.json",
		"build:electron:linux": "electron-builder -l --config electron-builder.config.json",
		"start": "svelte-kit start"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.34",
		"@sveltejs/kit": "^1.0.0-next.357",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.2.2",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.1",
		"electron": "^19.0.7",
		"electron-builder": "^23.1.0",
		"electron-connect": "^0.6.3",
		"electron-packager": "^15.5.1",
		"electron-updater": "^5.0.5",
		"eslint": "^8.19.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"heroicons": "^1.0.6",
		"lodash.debounce": "^4.0.8",
		"postcss": "^8.4.14",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.48.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.4"
	},
	"dependencies": {
		"electron-serve": "^1.1.0",
		"electron-store": "^8.0.2",
		"sanitize-filename": "^1.6.3",
		"tree-kill": "^1.2.2",
		"yt-search": "^2.10.3",
		"ytdl-core": "^4.11.0"
	}
}
